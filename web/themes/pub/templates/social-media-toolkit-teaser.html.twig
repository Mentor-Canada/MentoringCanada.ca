{# @var v \Drupal\app\Views\SocialMediaToolkitTeaserView #}
<a class="social-media-toolkit-teaser card-teaser" href="{{ v.url }}">
  <div class="card-teaser-image">
    <div class="card-teaser-image-background" style="background-image: url('{{ v.image }}')"></div>
  </div>
  <div class="card-teaser-meta">
    <div class="card-title">{{ v.title }}</div>
    <div class="card-chin">{{ v.date }}</div>
  </div>
</a>

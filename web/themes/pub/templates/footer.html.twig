{# @var v \Drupal\app\Views\FooterView #}
{% if v.display %}
  <footer>

    <div class="footer-flex">

      <div class="footer-menu">
        <h6>{{ v.gs.addressName }}</h6>
        <ul>
          <li>
            <a href="{{ v.baseUrl }}/{{ v.lang }}">{{ "Home"|t }}</a>
          </li>
          {% for item in v.menu %}
            <li>
              <a href="{{ item.url }}">{{ item.title }}</a>
            </li>
          {% endfor %}
        </ul>
      </div>

      <div class="footer-contact">
        <h6>{{ "Contact Us"|t }}</h6>
        <div class="footer-contact-item">
          <a href="{{ v.gs.phoneLink }}">{{ v.gs.phoneDisplayWithExtension }}</a>
        </div>
        <div class="footer-contact-item">
          <a href="{{ v.gs.emailLink }}">{{ v.gs.emailDisplay }}</a>
        </div>
      </div>

    </div>

    <div class="footer-flex">

      <div class="footer-newsletter">
        <h6>{{ "Subscribe to our newsletter!"|t }}</h6>
        <a href="{{ path('entity.node.canonical', {'node': 823}) }}">
          <button class="input-height">{{ "Subscribe"|t }}</button>
        </a>
      </div>


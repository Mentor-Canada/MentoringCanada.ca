{# @var v \Drupal\app\Views\EventView #}
{# @var pillarSection \Drupal\app\Views\ParagraphContentSectionView #}
{% set pillarSection = v.sections.1 %}


{% if v.banner.hasImage %}
{{ v.banner|raw }}
{% endif %}

<div id="content">
  <article>

    {{ v.header|raw }}

    {% if messages %}
      <div class="messages" data-content-width="standard">
        {{ messages|raw }}
      </div>
    {% endif %}

    <section class="event-when-where-section {% if (not v.hasEventOptions) %}no-event-options{% endif %}"
             data-section-zebra="odd"
    >
      <div data-padding-standard="horizontal">
        <div data-content-width="{{ v.eventWhereWhenWidth }}">
          <div class="event-when-where-label">
            {{ "When"|t }}
          </div>
          <div class="event-when-where-detail">
            <div class="date-time-range">{{ v.pageSectionDate.dateTimeRange }}</div>
            <div class="date-time-range-timezone">{{ v.pageSectionDate.timezone }}</div>
          </div>
          <div class="event-when-where-label">
            {{ "Details"|t }}
          </div>
          <div class="event-when-where-detail">
            {{ v.pageSectionDetails }}
          </div>
        </div>
      </div>
    </section>

    {% if not v.postEventContent %}
      {{ v.sections.0|raw }}

      <section class="content-section pillars-section" data-section-id="{{ pillarSection.id }}" data-section-zebra="odd">
        <div data-padding-standard="horizontal">
          <div data-content-width="full">

            <div class="pillar-section-intro typography" data-content-width="standard">{{ pillarSection.blocks.0.column1|raw }}</div>

            <div class="pillar-wrapper">

              <div class="pillar pillar-1 anim-view" data-anim-view-chain-delay="500">
                <div class="svg-wrapper">
                  <svg id="svg-pillar-1" xmlns="http://www.w3.org/2000/svg" width="80" height="60" viewBox="0 0 80 60">
                    <linearGradient id="svg_pillar_1_gradient_1" gradientUnits="userSpaceOnUse" x1="27.5346" y1="1.5304" x2="27.5346" y2="61.5304" gradientTransform="matrix(1 0 0 1 -1.5304 -1.5304)">
                      <stop  offset="0" style="stop-color:#D91C1C"/>
                      <stop  offset="1" style="stop-color:#A60000"/>
                    </linearGradient>
                    <path id="svg-pillar-1-anim-1" fill="url(#svg_pillar_1_gradient_1)" d="M30.02,11.8C14.62,2.67,3.91-1.6,1.21,0.55C-2.15,2.77,2.5,11.42,3,30C2.5,48.58-2.15,57.23,1.21,59.45c2.71,2.15,13.42-2.12,28.81-11.25C59.34,29.91,59.34,30.09,30.02,11.8z"/>
                    <linearGradient id="svg_pillar_1_gradient_2" gradientUnits="userSpaceOnUse" x1="53.9958" y1="-9.094947e-13" x2="53.9958" y2="60">
                      <stop  offset="0" style="stop-color:#FFAC26"/>
                      <stop  offset="1" style="stop-color:#D91C1C"/>
                    </linearGradient>
                    <path id="svg-pillar-1-anim-2" fill="url(#svg_pillar_1_gradient_2)" d="M58.01,11.8C42.62,2.67,31.91-1.6,29.2,0.55C25.85,2.77,30.5,11.42,30.99,30c-0.5,18.58-5.15,27.23-1.79,29.45c2.71,2.15,13.42-2.12,28.81-11.25C87.33,29.91,87.33,30.09,58.01,11.8z"/>
                  </svg>
                </div>
                <div class="typography">
                  {{ pillarSection.blocks.1.column1|raw }}
                </div>
              </div>

              <div class="pillar pillar-2 anim-view" data-anim-view-chain-delay="500">
                <div class="svg-wrapper">
                  <svg id="svg-pillar-2" xmlns="http://www.w3.org/2000/svg" width="80" height="60" viewBox="0 0 80 60">
                    <linearGradient id="svg_pillar_2_gradient_1" gradientUnits="userSpaceOnUse" x1="40" y1="0" x2="40" y2="60">
                      <stop  offset="0" style="stop-color:#FFAC26"/>
                      <stop  offset="1" style="stop-color:#E85520"/>
                    </linearGradient>
                    <path id="svg-pillar-2-anim-1" fill="url(#svg_pillar_2_gradient_1)" d="M10.19,0.92L10.19,0.92L8.06,1.46L7.42,1.67l-1.5,0.64L4.59,3.1L3.08,4.44L2.46,5.23L1.67,6.59L0.9,8.7L0.66,9.71l-0.31,1.94l-0.21,2.52l-0.11,3.05L0,24.07l0.04,18.71l0.11,3.05l0.21,2.52l0.31,1.94l0.41,1.6l0.6,1.54l0.78,1.34l1.1,1.28l1.04,0.85l1.34,0.8l2.13,0.85l2.13,0.54l2.13,0.35l2.13,0.23l4.33,0.24L27.41,60l29.63-0.03l6.05-0.16l2.64-0.18l3.46-0.44l1.57-0.33l1.82-0.54l1.5-0.64l1.33-0.79l1.51-1.34l0.62-0.79l0.79-1.36l0.77-2.12l0.24-1.01l0.31-1.94l0.21-2.52l0.11-3.05L80,35.93l-0.04-18.71l-0.11-3.05l-0.21-2.52l-0.31-1.94l-0.41-1.6l-0.6-1.54l-0.78-1.34l-1.1-1.28L75.41,3.1l-1.34-0.8l-2.13-0.85l-2.13-0.54l-2.13-0.35l-2.13-0.23l-4.33-0.24L52.59,0L22.96,0.03l-6.05,0.16l-2.64,0.18L10.81,0.8L10.19,0.92"/>
                    <linearGradient id="svg_pillar_2_gradient_2" gradientUnits="userSpaceOnUse" x1="41.5" y1="15" x2="41.5" y2="45">
                      <stop  offset="0" style="stop-color:#FFDC00"/>
                      <stop  offset="1" style="stop-color:#FBB03B"/>
                    </linearGradient>
                    <path id="svg-pillar-2-anim-2" fill="url(#svg_pillar_2_gradient_2)" d="M43.46,20.95C27.78,11.89,27.78,11.89,27.78,30s0,18.11,15.68,9.05S59.14,30,43.46,20.95z"/>
                  </svg>
                </div>
                <div class="typography">
                  {{ pillarSection.blocks.2.column1|raw }}
                </div>
              </div>

              <div class="pillar pillar-3 anim-view" data-anim-view-chain-delay="500">
                <div class="svg-wrapper">
                  <svg id="svg-pillar-3" xmlns="http://www.w3.org/2000/svg" width="75" height="75" viewBox="0 0 75 75">
                    <radialGradient id="svg_pillar_3_gradient_1" cx="37.5" cy="37.5" r="37.5" gradientUnits="userSpaceOnUse">
                      <stop  offset="0" style="stop-color:#33CEBF"/>
                      <stop  offset="1" style="stop-color:#009AA6"/>
                    </radialGradient>
                    <path id="svg-pillar-3-anim-1" fill="url(#svg_pillar_3_gradient_1)" d="M37.5,0C16.79,0,0,16.79,0,37.5C0,58.21,16.79,75,37.5,75S75,58.21,75,37.5C75,16.79,58.21,0,37.5,0zM37.5,67.5c-16.57,0-30-13.43-30-30c0-16.57,13.43-30,30-30s30,13.43,30,30C67.5,54.07,54.07,67.5,37.5,67.5z"/>
                    <path id="svg-pillar-3-anim-2" fill="url(#svg_pillar_3_gradient_1)" d="M37.5,15C25.07,15,15,25.07,15,37.5S25.07,60,37.5,60S60,49.93,60,37.5S49.93,15,37.5,15z M37.5,52.5c-8.28,0-15-6.72-15-15c0-8.28,6.72-15,15-15s15,6.72,15,15C52.5,45.78,45.78,52.5,37.5,52.5z"/>
                    <circle id="svg-pillar-3-anim-3" fill="url(#svg_pillar_3_gradient_1)" cx="37.5" cy="37.5" r="7.5"/>
                  </svg>
                </div>
                <div class="typography">
                  {{ pillarSection.blocks.3.column1|raw }}
                </div>
              </div>

              <div class="pillar pillar-4 anim-view" data-anim-view-chain-delay="500">
                <div class="svg-wrapper">
                  <svg id="svg-pillar-4" xmlns="http://www.w3.org/2000/svg" width="88" height="60" viewBox="0 0 88 60">
                    <linearGradient id="svg_pillar_4_gradient_1" gradientUnits="userSpaceOnUse" x1="63.2556" y1="60" x2="63.2556" y2="-9.094947e-13">
                      <stop  offset="0" style="stop-color:#003459"/>
                      <stop  offset="1" style="stop-color:#0061A6"/>
                    </linearGradient>
                    <path id="svg-pillar-4-anim-1" fill="url(#svg_pillar_4_gradient_1)" d="M79.92,16.67c0,9.2-7.46,16.67-16.67,16.67s-16.67-7.46-16.67-16.67S54.05,0,63.26,0S79.92,7.46,79.92,16.67z M63.26,35c-12.5,0-22.85,9.17-24.7,21.14C38.24,58.17,39.8,60,41.85,60h42.81c2.05,0,3.61-1.83,3.3-3.86C86.11,44.17,75.75,35,63.26,35z"/>
                    <linearGradient id="svg_pillar_4_gradient_2" gradientUnits="userSpaceOnUse" x1="24.7444" y1="60" x2="24.7444" y2="-9.094947e-13">
                      <stop  offset="0" style="stop-color:#0061A6"/>
                      <stop  offset="1" style="stop-color:#00B5C2"/>
                    </linearGradient>
                    <path id="svg-pillar-4-anim-2" opacity="0.9" fill="url(#svg_pillar_4_gradient_2)" d="M41.41,16.67c0,9.2-7.46,16.67-16.67,16.67S8.08,25.87,8.08,16.67S15.54,0,24.74,0S41.41,7.46,41.41,16.67z M24.74,35c-12.5,0-22.85,9.17-24.7,21.14C-0.27,58.17,1.29,60,3.34,60h42.81c2.05,0,3.61-1.83,3.3-3.86C47.59,44.17,37.24,35,24.74,35z"/>
                  </svg>
                </div>
                <div class="typography">
                  {{ pillarSection.blocks.4.column1|raw }}
                </div>
              </div>

            </div>

          </div>
        </div>
      </section>

    {% else %}

    <section class="event-form-expired-section"
             data-section-zebra="odd"
    >
      <div data-padding-standard="horizontal">
        <div data-content-width="standard">
          <div class="typography">
            {{ v.postEventContent|raw }}
          </div>
        </div>
      </div>
    </section>

    {% endif %}


    {% if v.activeEvent %}
    {% if v.form is not null %}

      <section class="form-section"
               data-section-zebra="even"
      >
        <div data-padding-standard="horizontal">
          <div data-content-width="standard">
            <div class="typography form-section-heading">
              <h2 class="visually-h5 form-section-heading-eyebrow">{{ "Event Registration"|t }}</h2>
              <h3 class="visually-h3 form-section-heading-title">{{ node.label }}</h3>
            </div>
            {{ v.form|raw }}
          </div>
        </div>
      </section>

    {% endif %}
    {% endif %}

    {% if v.moreItems|length %}
    <section class="more-items-section">
      <div data-padding-standard="horizontal">
        <div data-content-width="standard">
          <div class="more-heading">
            <div class="typography">
              <h2>{{ "More events"|t }}</h2>
            </div>
          </div>
          <div class="items-wrapper">
            {% for item in v.moreItems %}
              {{ item|raw }}
            {% endfor %}
          </div>
        </div>
      </div>
    </section>
    {% endif %}

  </article>
</div>

<script>
  if(!window.pageData) window.pageData = {};
  window.pageData.type = "event";
</script>

